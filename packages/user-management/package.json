{
  "name": "@zetapush/user-management",
  "version": "0.31.4",
  "description": "Zeta Push Next",
  "browser": "dist/zetapush-user-management.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "index.d.ts"
  ],
  "repository": "https://github.com/zetapush/zetapush",
  "author": "ci@zetapush.com",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist es .deploy .esdoc",
    "build:commonjs": "tsc --outDir lib",
    "build:commonjs:test": "tsc --outDir lib",
    "build:es": "tsc --module ES2015 --moduleResolution Node --outDir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -i src/index.ts -o dist/zetapush-user-management.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -i src/index.ts -o dist/zetapush-user-management.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "test": "npm run clean && npm run build:commonjs:test && node_modules/.bin/nyc ts-node -r source-map-support/register node_modules/.bin/jasmine",
    "prepublish": "npm run clean && npm run build",
    "build:api-doc": "typedoc --out ./docs ./src/index.ts"
  },
  "dependencies": {
    "@zetapush/common": "0.31.4",
    "@zetapush/core": "0.31.4",
    "@zetapush/client": "0.31.4",
    "@zetapush/http-server": "0.31.4",
    "@zetapush/platform-legacy": "0.31.4",
    "axios": "0.19.0-beta.1",
    "class-validator": "0.8.5"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.8",
    "@zetapush/testing": "0.31.4",
    "@zetapush/worker": "0.31.4",
    "axios-mock-adapter": "1.15.0",
    "catharsis": "0.8.9",
    "cross-env": "5.2.0",
    "jasmine": "3.1.0",
    "jasmine-reporters": "2.3.1",
    "jasmine-ts": "0.2.1",
    "jasmine-ts-console-reporter": "3.1.1",
    "nyc": "12.0.2",
    "reflect-metadata": "0.1.12",
    "request": "2.85.0",
    "rimraf": "2.6.2",
    "rollup": "0.64.1",
    "rollup-plugin-bundle-size": "1.0.1",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-size-snapshot": "0.6.1",
    "rollup-plugin-typescript": "0.8.1",
    "rollup-plugin-uglify": "4.0.0",
    "rollup-watch": "4.3.1",
    "source-map-support": "0.5.8",
    "taffydb": "2.7.3",
    "ts-jsdoc": "3.0.1",
    "ts-mockito": "2.3.0",
    "ts-node": "7.0.1",
    "ts2jsdoc": "0.1.2",
    "tsdoc": "0.0.4",
    "typedoc": "0.11.1",
    "typescript": "3.0.1",
    "uglify-es": "3.3.10",
    "underscore": "1.9.1",
    "mockttp": "0.9.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "html",
      "cobertura",
      "lcovonly"
    ],
    "all": true
  }
}
