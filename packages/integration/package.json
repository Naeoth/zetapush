{
  "name": "@zetapush/integration",
  "private": true,
  "scripts": {
    "test:ci": "npm run zp:clean && npm run zp:create-project && npm run zp:deploy && npm run zp:test",
    "zp:clean": "rimraf project",
    "zp:create-project": "npm init @zetapush project --developer-login $ZETAPUSH_DEVELOPER_LOGIN --developer-password $ZETAPUSH_DEVELOPER_PASSWORD",
    "zp:deploy": "cd project && npm run deploy",
    "zp:test": "node index.js"
  },
  "dependencies": {
    "@zetapush/cometd": "*",
    "@zetapush/core": "*",
    "rimraf": "^2.6.2"
  }
}
